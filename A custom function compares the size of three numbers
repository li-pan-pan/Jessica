#include<stdio.h>
void swap(int *a, int *b)
{
	int temp;
	temp = *a;
	*a = *b;
	*b = temp;
}
int main()
{
	int a, b, c;
	printf("请输入三个数：\n");
	scanf("%d%d%d", &a, &b, &c);
	if (a < b)
	{
		swap(&a, &b);
	}
	if (b < c)
	{
		swap(&b, &c);
	}
	if (a < b)
	{
		swap(&a, &b);
	}
	printf("a=%d,b=%d,c=%d\n", a, b, c);
}
