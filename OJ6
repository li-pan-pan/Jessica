struct ListNode* FindKthToTail(struct ListNode* pListHead, int k ) {
  // write code here
  struct ListNode* slow,* fast;
  slow=pListHead;
  fast=pListHead;
  while(k--)
  {
    if(fast==NULL)
    {
      return NULL;
    }
    fast=fast->next;
  }
  while(fast)
  {
    slow=slow->next;
    fast=fast->next;
  }
  return slow;
}
