#include<iostream>
#include<string>
using namespace std;
int main()
{
    string str;
    getline(cin,str);
    int count=0;
    int len=str.size();
    for(int i=(len-1);i>=0;i--)
    {
        if(str[i]!=' ')
        {
            count++;
        }
        else{
            break;
        }
        
    }
    cout<<count<<endl;
    return 0;
}
