#include<stdio.h>
int sort(int *a,int n)
{
	int end = n;
	while (end>0)
	{
		int m = 0;
		int i = 0;
		while (i < end-1)
		{
			if (a[i] < a[i + 1])
			{
				a[i] = a[i] ^ a[i + 1];
				a[i + 1] = a[i] ^ a[i + 1];
				a[i] = a[i] ^ a[i + 1];
				m = 1;
			}
			++i;
		}
		if (m == 0)
		{
			break;
		}
		--end;
	}
	return 0;
}
int print(int *a,int n)
{
	int i;
	for (i = 0; i < n; ++i)
	{
		printf("%d ", a[i]);
	}
	printf("\n");
	return 0;
}
int main()
{
	int a[] = { 3, 4, 2, 6, 6, 7, 8, 4, 5, 6, 7, 9, };
	int n = sizeof(a) / sizeof(int);
	print(a, n);
	sort(a, n);
	print(a, n);
	return 0;
	
}
