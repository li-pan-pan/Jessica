#include"å †.h"


void Heaptest()
{
	Heap hp;
	int a[] = { 63, 34, 56, 35, 54, 24, 87, 67, 98, 78};
	HeapCreate(&hp,a,10);
	while (!HeapEmpty(&hp))
	{
		printf("%d  ", HeapTop(&hp));
		HeapPop(&hp);
	}
	printf("\n");
	HeapSort(a, 10);
	for (int i = 0; i < 10; ++i)
	{
		printf("%d  ", a[i]);
	}
	printf("\n");
	PrintTopK(a, 10, 3);
	//HeapDestory(&hp);
}

int main()
{
	Heaptest();
	return 0;
}
